{
  "discord": {
    "intents": ["Guilds", "GuildMessages", "MessageContent"],
    "fetchLimit": 100,
    "maxMessagesPerChannel": 10000,
    "maxAgeDays": 90,
    "channelAllowlist": [],
    "channelBlocklist": [],
    "excludeChannelTypes": [2, 4, 13, 15],
    "excludeChannelNames": []
  },
  "scraper": {
    "delayBetweenChannels": 1000,
    "delayBetweenRequests": 100,
    "retryAttempts": 3,
    "retryDelayMs": 1000,
    "backoffMultiplier": 2
  },
  "ai": {
    "provider": "openrouter",
    "model": "moonshotai/kimi-k2",
    "maxTokens": 8192,
    "batchSize": 30,
    "maxTokensPerBatch": 6000,
    "retryAttempts": 3,
    "retryDelayMs": 1000,
    "stages": {
      "filter": { "enabled": true },
      "categorize": { "enabled": true },
      "summarize": { "enabled": true },
      "extract": { "enabled": true },
      "format": { "enabled": true }
    }
  },
  "privacy": {
    "anonymizeInPrompts": true,
    "anonymizeInExports": true,
    "consentExpiryDays": 365
  },
  "database": {
    "path": "./data/discord.db"
  },
  "export": {
    "defaultFormat": "json",
    "outputDir": "./exports"
  },
  "logging": {
    "level": "info",
    "format": "text"
  }
}
