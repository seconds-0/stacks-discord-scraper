  "author": "Stacks Labs",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "ajv": "^8.17.1",
    "better-sqlite3": "^11.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "openai": "^6.10.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "prettier": "^3.4.2"
