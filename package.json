{
  "name": "stacks-discord-scraper",
  "version": "1.0.0",
  "description": "Discord scraper and AI content pipeline for Stacks Labs marketing",
  "main": "src/cli/index.js",
  "bin": {
    "stacks-discord": "./src/cli/index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node src/cli/index.js",
    "scrape": "node src/cli/index.js scrape",
    "scrape:full": "node src/cli/index.js scrape --full",
    "scrape:incremental": "node src/cli/index.js scrape --incremental",
    "process": "node src/cli/index.js process",
    "export": "node src/cli/index.js export",
    "db:init": "node src/cli/index.js db init",
    "db:stats": "node src/cli/index.js db stats",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/",
    "format": "prettier src/ --write",
    "format:check": "prettier src/ --check"
  },
  "keywords": [
    "discord",
    "scraper",
    "stacks",
    "marketing",
    "ai",
    "content-pipeline"
